@model List<Food>
<div class="row">
    <div class="col-12">
        <h2>Post List</h2>
        <a href="/posts/create" class="btn btn-primary">New Post</a>
        <hr>
        <table class="table">
            <thead>
                <tr>
                    <th style="width: 160px;">Image</th>
                    <th>Title</th>
                    <th style="width: 160px;">IsActive</th>
                    <th style="width: 160px;">Setting</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model){
                    <tr>
                        
                        <td>@item.Title</td>
                        
                        <td>
                            <a href="/posts/edit/@item.FoodId" class="btn btn-primary">Edit</a>
                            <a href="#" class="btn btn-danger">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

